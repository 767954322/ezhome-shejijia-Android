        <attr name="mcv_calendarMode" format="enum">
            <enum name="month" value="0" />
            <enum name="week" value="1" />
        </attr>
    </declare-styleable>

    <!--for crop-->
    <declare-styleable name="CropView">
        <!-- Viewport ratio used to compute height based off view width -->
        <attr name="ViewportRatio" format="float" />
        <!-- Viewport overlay color -->
        <attr name="ViewportOverlayColor" format="color" />
        <!-- Viewport overlay padding -->
        <attr name="ViewportOverlayPadding" format="dimension" />
        <!-- Maximum zoom level -->
        <attr name="MaxScale" format="float" />
        <!-- Minimum  zoom level -->
        <attr name="MinScale" format="float" />
    </declare-styleable>
</resources>
