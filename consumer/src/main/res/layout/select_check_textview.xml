<?xml version="1.0" encoding="utf-8"?>
<CheckedTextView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ctv_select"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:checkMark="@drawable/selset_project_checkbox"
    android:gravity="center_vertical"
    android:lineSpacingExtra="@dimen/size_7"
    android:paddingTop="@dimen/size_13"
    android:paddingBottom="@dimen/size_13"
    android:paddingEnd="@dimen/size_16"
    android:paddingStart="@dimen/size_16"
    android:textAppearance="?android:attr/textAppearanceListItemSmall"
    android:textColor="@color/text_item_name"
    android:textSize="@dimen/font_15">
</CheckedTextView>
